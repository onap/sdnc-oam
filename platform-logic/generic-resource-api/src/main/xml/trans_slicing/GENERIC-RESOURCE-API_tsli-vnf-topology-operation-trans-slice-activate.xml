	resource="vpn-binding"
	    key="vpn-binding.vpn-id = $tmp.aai.connectivityB.relationship-list.relationship[0].relationship-data[0].relationship-value"
		local-only="false" 
		pfx="tmp.aai.vpn-bindingB" >
                                                        <outcome value='success'>
                                                            <block atomic="true">
                                                                <set>
