                                                                                            <block>
                                                                                                <set>
                                                                                                    <parameter name='tmpo.te-topo-id' value='`$network.te-topology-id`' />
                                                                                                </set>
                                                                                            </block>
                                                                                        </outcome>
                                                                                    </switch>
