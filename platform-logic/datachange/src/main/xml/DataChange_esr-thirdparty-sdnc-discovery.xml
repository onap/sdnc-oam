                                                                    <outcome value='true'>
                                                                        <block atomic='true'>
                                                                            <for index='netidx' start='0' end='`$topology.networks.network_length`' >
                                                                                <block>
                                                                                    <set>
                                                                                        <parameter name='network.' value='' />
                                                                                    </set>
                                                                                    <set>
                                                                                        <parameter name='network.' value='`$topology.networks.network[$netidx].`' />
                                                                                    </set>
                                                                                    <set>
